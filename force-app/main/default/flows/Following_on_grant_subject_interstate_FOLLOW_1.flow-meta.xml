    <formulas>
        <name>additionalNameFormula</name>
        <dataType>String</dataType>
        <expression>if({!detailSurname}&lt;&gt;&quot;&quot;,&quot; otherwise &quot;+{!detailForename}+&quot; &quot;+{!detailSurname},&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>concatenateAddressOfDeathFormula</name>
        <dataType>String</dataType>
        <expression>{!Enterfulladdressofplaceofdeathofsubject.street}+&quot;, &quot;+{!Enterfulladdressofplaceofdeathofsubject.city}+&quot;, &quot;+{!Enterfulladdressofplaceofdeathofsubject.postalCode}+&quot;, &quot;+{!Enterfulladdressofplaceofdeathofsubject.province}+&quot;, &quot;+{!Enterfulladdressofplaceofdeathofsubject.country}</expression>
    </formulas>
    <formulas>
        <name>daysAdd28</name>
        <dataType>Date</dataType>
        <expression>TODAY () + 28</expression>
    </formulas>
    <formulas>
        <name>sameLocalityFormula</name>
        <dataType>String</dataType>
        <expression>IF({!Subject_died_in_same_locality_as_last_residence1}=&quot;Yes&quot;, &quot;True&quot;, &quot;False&quot;)</expression>
    </formulas>
    <formulas>
        <name>subjectGenderFormula</name>
        <dataType>String</dataType>
        <expression>if({!Gender_of_subject1}=&quot;Male&quot;, &quot;True&quot;, &quot;False&quot;)</expression>
    </formulas>
    <formulas>
        <name>titleFormulaAd</name>
        <dataType>Boolean</dataType>
        <expression>if(BEGINS({!title}, &quot;Ad&quot;),True,False)</expression>
    </formulas>
    <formulas>
        <name>titleFormulaEx</name>
        <dataType>Boolean</dataType>
        <expression>if(BEGINS({!title}, &quot;Ex&quot;),True,False)</expression>
    </formulas>
